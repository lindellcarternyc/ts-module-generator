{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "strict": true,
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@Interfaces": [
        "interfaces"
      ],
      "@Interfaces/*": [
        "interfaces/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "testDir"
  ]
}